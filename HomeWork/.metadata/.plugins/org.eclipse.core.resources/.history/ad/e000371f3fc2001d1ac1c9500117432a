package SecondWork;
import java.util.Scanner;
public class Problem_five {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		int y  = sc.nextInt();
		int z = sc.nextInt();
		int tmax = Math.max(Math.max(x, y), z);
		System.out.println(tmax);
	}
}
