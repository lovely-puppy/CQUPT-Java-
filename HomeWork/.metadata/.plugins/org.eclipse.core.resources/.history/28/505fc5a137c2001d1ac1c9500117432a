package SecondWork;
import java.util.Scanner;
public class Problem_four {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int x = sc.nextInt();
		if (get(x)) {
			System.out.print("yes");
		} else {
			System.out.print("no");
		}
	}
	static boolean get(int x)
	{
		int y = x;
		int n = 0;
		while (y != 0) {
			n ++ ;
			y /= 10;
		}
		System.out.print("----" + n + "----");
	}
	return false;
}